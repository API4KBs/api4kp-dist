  <owl:NamedIndividual rdf:about="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#b4b94452-af12-352d-832f-cdec7fd11234">
    <terms:identifier>b4b94452-af12-352d-832f-cdec7fd11234</terms:identifier>
    <rdfs:label>natural technique</rdfs:label>
    <skos:definition>technique that requires involvement of a person</skos:definition>
    <rdfs:isDefinedBy>https://www.omg.org/spec/API4KP/api4kp-kao/NaturalTechnique</rdfs:isDefinedBy>
    <skos:broader rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
    <skos:prefLabel>natural technique</skos:prefLabel>
    <skos:inScheme rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#294f76a3-f64f-3f80-b269-2f73c0159e18"/>
    <skos:note>A natural technique is one that requires some aspect of human-nature, or creativity, spontaneity, ingenuity, or other complex intellectual reasoning, such that a computational engine is incapable of accomplishing a similar task using this technique.</skos:note>
    <skos:notation>NaturalTechnique</skos:notation>
    <skos:broaderTransitive rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#8a717c26-8031-3210-99b1-a3891897b2c5">
    <rdfs:label>qualitative technique</rdfs:label>
    <rdfs:isDefinedBy>https://www.omg.org/spec/API4KP/api4kp-kao/QualitativeTechnique</rdfs:isDefinedBy>
    <skos:inScheme rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#294f76a3-f64f-3f80-b269-2f73c0159e18"/>
    <skos:broader rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#c710da1c-bc8f-34d3-9e5b-3bb26cdf334e"/>
    <terms:identifier>8a717c26-8031-3210-99b1-a3891897b2c5</terms:identifier>
    <skos:prefLabel>qualitative technique</skos:prefLabel>
    <skos:definition>computational technique that is largely holistic, logic-based, symbolic, and/or non-statistical in nature, that uses qualitative characteristics in analyses</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>QualitativeTechnique</skos:notation>
    <skos:broaderTransitive rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
  </owl:NamedIndividual>
