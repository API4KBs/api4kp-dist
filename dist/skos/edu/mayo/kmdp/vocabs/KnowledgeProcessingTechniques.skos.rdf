    <skos:definition>technique that involves question answering and/or requesting another agent to perform some aspect of a task, from which some result can be obtained</skos:definition>
    <skos:notation>QueryTechnique</skos:notation>
    <skos:broaderTransitive rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
    <rdfs:label>query technique</rdfs:label>
    <rdfs:isDefinedBy>https://www.omg.org/spec/API4KP/api4kp-kao/QueryTechnique</rdfs:isDefinedBy>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
