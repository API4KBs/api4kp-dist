    <skos:broaderTransitive rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
    <skos:prefLabel>interactive technique</skos:prefLabel>
    <skos:broader rdf:resource="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#9c984f4d-d403-3181-b7a9-0d5402d73c04"/>
    <skos:notation>InteractiveTechnique</skos:notation>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="https://www.omg.org/spec/API4KP/20200801/taxonomy/KnowledgeProcessingTechnique#221f0704-2eb6-3767-806c-bb70fdb5fde7">
